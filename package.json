{
  "name": "event-action",
  "version": "1.0.1",
  "description": "一个简单的发布订阅器",
  "keywords": [
    "event",
    "emitter",
    "typescript"
  ],
  "type": "module",
  "main": "dist/event.cjs.js",
  "module": "dist/event.esm.js",
  "browser": "dist/event.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs && tsc -p ./tsconfig.type.json",
    "dev": "rollup -w -c --bundleConfigAsCjs",
    "tsc": "tsc -p ./tsconfig.type.json"
  },
  "files": [
    "dist/**/*",
    "types/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haijee/event-action.git"
  },
  "author": "郭海杰",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haijee/event-action/issues"
  },
  "homepage": "https://github.com/haijee/event-action/#readme",
  "devDependencies": {
    "rollup": "^3.25.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  }
}
